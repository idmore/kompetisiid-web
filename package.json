{
  "name": "kompetisi-indonesia",
  "version": "4.4.0-beta.0",
  "description": "platform untuk mengikuti dan berbagi kompetisi",
  "main": "dist-server/server/index.js",
  "scripts": {
    "start": "npm run server:dev",
    "prepublish": "npm run build:prod",
    "build:watch": "NODE_ENV=development webpack --watch --progress",
    "build:prod": "rimraf dist-server && rimraf dist-client && npm run build-client:prod && npm run build-server:prod",
    "lint": "eslint src --ext .js --ext .jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "yarn build:prod",
    "server:dev": "NODE_ENV=development nodemon --watch src/ --watch dist-client/ --watch public/ --exec 'node -r dotenv/config -r babel-register' ./src/server",
    "server:prod": "NODE_ENV=production nodemon --exec 'node -r dotenv/config' ./dist-server/server",
    "build-client:prod": "NODE_ENV=production webpack --progress",
    "build-server:prod": "NODE_ENV=production babel src -d dist-server --ignore '**/*_test.js' --copy-files",
    "zip": "rimraf dist-server.zip && rimraf dist-client.zip && bestzip dist-server.zip dist-server && bestzip dist-client.zip dist-client",
    "release": "standard-version",
    "release:beta": "standard-version -p beta"
  },
  "pre-commit": "lint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idmore/kompetisiid-web.git"
  },
  "keywords": [
    "kompetisi",
    "indonesia"
  ],
  "author": "yussan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/idmore/kompetisiid-web/issues"
  },
  "homepage": "https://github.com/idmore/kompetisiid-web#README",
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "bluebird": "3.4.6",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-alpha.2",
    "copy-to-clipboard": "^3.0.8",
    "express": "4.14.0",
    "formidable": "1.1.1",
    "fs": "0.0.1-security",
    "history": "4.6.1",
    "moment": "2.17.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "2.8.1",
    "react-transition-group": "^2.2.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "request": "2.75.0",
    "string-manager": "^1.1.1",
    "superagent": "2.3.0",
    "xml": "1.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "6.24.0",
    "babel-core": "6.17.0",
    "babel-jest": "16.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-register": "6.14.0",
    "babili": "0.0.12",
    "bestzip": "^1.1.4",
    "dotenv": "2.0.0",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "express-debug": "^1.1.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.0",
    "pre-commit": "^1.2.2",
    "redux-logger": "2.6.1",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "webpack": "^3.6.0"
  }
}
